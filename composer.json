{
    "name": "koka/phpixie-project",
    "type": "project",
    "description": "PHPixie advanced skeleton project",
    "license": "MIT",
    "keywords": [
        "phpixie",
        "project"
    ],
    "authors": [
        {
            "name": "Kostya Nemchenko",
            "email": "kostya.nemchenko@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": [
                "src/",
                "tests/",
                "bundles/app/src/",
                "bundles/app/tests/"
            ]
            
        }
    },
    "require": {
        "php": ">=5.4.0",
        "phpixie/default-bundle": "~3.1",
        "phpixie/bundle-framework": "~3.1",
        "koka/flash": "dev-master",
        "koka/phpixie-bootstrap3-bundle": "dev-master"
    },
    "require-dev": {
        "phpixie/test": "3.*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.*-dev"
        }
    }
}
